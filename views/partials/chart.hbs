<div class="tradingview-widget-container">
<!--  <div id="tradingview_cccaf"></div>-->
  <div class="tradingview-widget-copyright">Chart Widget by TradingView</div>
  <script type="text/javascript" src="https://s3.tradingview.com/tv.js"></script>
  <script type="text/javascript">
    new TradingView.widget(
        {
          "width": 800,
          "height": 450,
          "symbol": "{{symbol}}",
          "interval": "D",
          "timezone": "Etc/UTC",
          "theme": "light",
          "style": "1",
          "locale": "en",
          "toolbar_bg": "#f1f3f6",
          "enable_publishing": false,
          "allow_symbol_change": true,
          "container_id": "tradingview_cccaf"
        }
    );
  </script>
</div>
<!-- TradingView Widget END -->