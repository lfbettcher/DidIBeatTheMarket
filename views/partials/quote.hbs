<section id="getQuote">
  <h2>Get a quick quote</h2>
  <form action="/" method="POST">
    <input type="text" name="stockSymbol" placeholder="stock symbol">
    <input type="submit" value="Submit">
    <!--  <input type="button" value="Reset">-->
  </form>
  <h3>Quotes</h3>
  <ul>
    {{#each stocks}}
    <li>The current price of {{stockSymbol}} is ${{stockData}}</li>
    {{/each}}
  </ul>
</section>